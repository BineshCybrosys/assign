<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="CustomProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">true</item>
        <!-- <item name="android:progressDrawable">@drawable/my_progress_indeterminate</item> -->
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">30dip</item>
        <item name="android:minWidth">60dip</item>
        <item name="android:maxWidth">100dip</item>
    </style>

    <!-- <style name="stBtnlayout">
        <item name="android:layout_width">80dip</item>
        <item name="android:layout_height">80dip</item>
    </style> -->

    <style name="display_style">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="border">
        <item name="android:layout_marginRight">2dip</item>
        <item name="android:layout_marginLeft">2dip</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:layout_marginBottom">2dip</item>
    </style>

    <style name="PreferencesTheme">
        <item name="android:background">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
        <item name="android:windowNoTitle">true</item>
    </style>

